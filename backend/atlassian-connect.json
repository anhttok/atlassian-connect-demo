{
  "key": "demo-drag-drop-macro",
  "name": "Demo drag and drop macro",
  "description": "This is a demo macro",
  "baseUrl": "{{localBaseUrl}}",
  "authentication": {
    "type": "jwt"
  },
  "lifecycle": {
    "installed": "/installed"
  },
  "scopes": ["READ", "WRITE"],
  "apiMigrations": {
    "signed-install": true
  },
  "modules": {
    "staticContentMacros": [
      {
        "url": "/macro/multiExcerpt?macroId={macro.id}&pageId={page.id}&pageVersion={page.version}",
        "description": {
          "value": "Multi excerpt"
        },
        "bodyType": "rich-text",
        "name": {
          "value": "Multi excerpt"
        },
        "key": "multi-excerpt-dynamic-content",
        "parameters": [
          {
            "identifier": "name",
            "name": {
              "value": "Name"
            },
            "type": "string",
            "required": true
          }
        ]
      },
      {
        "url": "/macro/multiExcerptIncludeView?macroId={macro.id}&pageId={page.id}&pageVersion={page.version}",
        "description": {
          "value": "Multi excerpt include"
        },
        "renderingMethod": "get",
        "bodyType": "none",
        "name": {
          "value": "Multi excerpt include"
        },
        "key": "multi-excerpt-include-content",
        "editor": {
          "url": "/macro/multiExcerptIncludeEditor",
          "editTitle": {
            "value": "Edit excerpt include"
          },
          "insertTitle": {
            "value": "Insert excerpt"
          }
        }
      }
    ]
  }
}
